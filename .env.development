# ProFish Development Environment (#31)
# Copy to .env and fill in real values

# ── Map ──────────────────────────────────────
MAPBOX_ACCESS_TOKEN=pk.dev_token_here

# ── Firebase ──────────────────────────────────
FIREBASE_WEB_API_KEY=dev_firebase_api_key
FIREBASE_PROJECT_ID=profish-dev

# ── Weather ───────────────────────────────────
# Open-Meteo: No key needed (free)
OPENWEATHER_API_KEY=dev_openweather_key

# ── Tides ─────────────────────────────────────
WORLDTIDES_API_KEY=dev_worldtides_key

# ── RevenueCat ────────────────────────────────
REVENUECAT_IOS_KEY=appl_dev_revenuecat_key
REVENUECAT_ANDROID_KEY=goog_dev_revenuecat_key

# ── Sentry ────────────────────────────────────
SENTRY_DSN=https://dev@sentry.io/profish-dev

# ── AI / ML ───────────────────────────────────
AI_SPECIES_API_URL=https://dev-api.profish.app/v1/species/identify

# ── Marketplace ───────────────────────────────
STRIPE_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxx
AMAZON_ASSOCIATE_TAG=profish-dev-20

# ── Feature Flags ─────────────────────────────
ENABLE_AR_MEASUREMENT=true
ENABLE_VOICE_LOGGING=true
ENABLE_AI_CHATBOT=true
ENABLE_MARKETPLACE=false

# ── Debug ─────────────────────────────────────
LOG_LEVEL=debug
SHOW_PERFORMANCE_OVERLAY=false
