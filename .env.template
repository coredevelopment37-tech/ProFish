# Environment template for local development
# Copy this file to `.env` and fill values (DO NOT commit secrets)

# ── Mapbox ───────────────────────────────────────────
MAPBOX_ACCESS_TOKEN=YOUR_MAPBOX_PUBLIC_TOKEN_HERE

# ── API ──────────────────────────────────────────────
API_BASE_URL=https://api.profish.app

# ── Firebase ─────────────────────────────────────────
FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY_HERE
FIREBASE_AUTH_DOMAIN=profish-app.firebaseapp.com
FIREBASE_PROJECT_ID=profish-app
FIREBASE_DATABASE_URL=
FIREBASE_STORAGE_BUCKET=profish-app.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=YOUR_SENDER_ID_HERE
FIREBASE_APP_ID=YOUR_APP_ID_HERE

# ── Google Sign-In ───────────────────────────────────
GOOGLE_WEB_CLIENT_ID=YOUR_GOOGLE_WEB_CLIENT_ID_HERE

# ── Sentry ───────────────────────────────────────────
SENTRY_DSN=

# ── Google Maps (Android) ────────────────────────────
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# ── AWS ──────────────────────────────────────────────
AWS_REGION=us-east-1
AWS_S3_BUCKET=profish-uploads

# ── WorldTides ───────────────────────────────────────
WORLDTIDES_API_KEY=YOUR_WORLDTIDES_API_KEY_HERE

# ── Environment ──────────────────────────────────────
NODE_ENV=development
